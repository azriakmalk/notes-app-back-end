{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console":"off",
        "no-trailing-spaces":"off",
        "linebreak-style":"off",
        "comma-dangle":"off",
        "quotes":"off"
    }
}
